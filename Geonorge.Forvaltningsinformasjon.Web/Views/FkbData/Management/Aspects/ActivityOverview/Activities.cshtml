@using Microsoft.AspNetCore.Mvc.Localization
@inject ApplicationSettings AppSettings
@inject IViewLocalizer Localizer

@{
    Layout = "../_Layout.cshtml";
    ViewData["currentTab"] = "Activities";
}

@section scripts {
<script>
    geonorge.BreadcrumbList.BreadcrumbList.setup("#breadcrumb-list", {
        breadcrumbs: [
            {
                name: 'Geonorge',
                url: '@AppSettings.UrlGeonorgeRoot'
            },
            {
                name: '@Localizer["Forvaltningsinformasjon"]',
                url: '/'
            },
            {
                name: '@Localizer["Aktivitetsoversikt"]',
                url: ''
            }
        ]
    });
</script>
}

