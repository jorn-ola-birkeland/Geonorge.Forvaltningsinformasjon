@using Microsoft.AspNetCore.Mvc.Localization
@using Geonorge.Forvaltningsinformasjon.Web.Models.Common
@inject ApplicationSettings AppSettings
@inject IViewLocalizer Localizer

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = Localizer["Dataflyt for Norge Digitalt Arealplankartdatabase (NAP)"];
    ViewData["tabs"] = new TabViewModel[] {
        new TabViewModel() { Caption = "Oppdateringsregime", Controller = "Geosynchronization", Aspect = true}
    };
}

@section scripts {
<script>
    geonorge.BreadcrumbList.BreadcrumbList.renderBreadcrumbs([
        {
            name: 'Geonorge',
            url: '@AppSettings.UrlGeonorgeRoot'
        },
        {
            name: '@Localizer["Forvaltningsinformasjon"]',
            url: '/'
        },
        {
            name: '@Localizer["Plandata"]',
            url: '/nap/info'
        },
        {
            name: '@Localizer["Dataflyt NAP"]',
            url: ''
        }
    ]);
</script>
}


