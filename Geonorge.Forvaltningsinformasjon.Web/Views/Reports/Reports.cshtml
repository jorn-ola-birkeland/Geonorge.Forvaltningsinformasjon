@using Microsoft.AspNetCore.Mvc.Localization
@model List<Geonorge.Forvaltningsinformasjon.Web.Models.Reports.Report>
@inject ApplicationSettings AppSettings
@inject IViewLocalizer Localizer

@section scripts {
<script>
    geonorge.BreadcrumbList.BreadcrumbList.renderBreadcrumbs([
        {
            name: 'Geonorge',
            url: '@AppSettings.UrlGeonorgeRoot'
        },
        {
            name: '@Localizer["Forvaltningsinformasjon"]',
            url: '/'
        },
        {
            name: 'Rapporter',
            url: '/rapport'
        },
        {
            name: '@Model.FirstOrDefault().Type',
            url: ''
        }
    ]);
</script>
}

<gn-table>
    <table>
        <thead>
            <tr>
                <th>Fylke</th>
                <th>Tidspunkt</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var report in Model)
            {
                <tr>
                    <td><a href="?rapport=@report.Type&fnr=@report.FylkesNummer">@report.FylkesNavn</a></td>
                    <td>@report.Tidspunkt</td>
                </tr>
            }
        </tbody>
    </table>
</gn-table>